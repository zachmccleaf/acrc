{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "target": "es5",
    "jsx": "react",
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "lib": ["es6", "dom"],
    "types": ["webpack-env"],
    "noImplicitAny": false,
  },
  "exclude": [
      "bin",
      "node_modules"
  ]
}
